{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "~/*": ["./*"]
    },
    "types": [
      "node",
      "@nuxtjs/i18n"
    ],
    "target": "ESNext",
    "module": "ESNext",
    "allowJs": true,
    "noEmit": true,
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules"
  ]
}
